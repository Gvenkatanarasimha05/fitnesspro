<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Community | FitnessPro</title>
    <script src="https://cdn.tailwindcss.com"></script>
</head>
<body class="bg-black text-gray-200">

<header class="bg-gray-900 p-6 flex justify-between items-center border-b border-gray-700">
    <h1 class="text-xl font-semibold tracking-wide">FitnessPro Community</h1>
    <nav>
        <ul class="flex space-x-6 text-gray-400">
            <li><a href="/dashboard" class="hover:text-white">Dashboard</a></li>
            <li><a href="/profile" class="hover:text-white">Profile</a></li>
            <li><a href="/" class="bg-gray-700 px-4 py-2 rounded-md hover:bg-gray-600">Logout</a></li>
        </ul>
    </nav>
</header>

<main class="max-w-2xl mx-auto mt-10 space-y-8">
    <!-- Influencer Posts -->
    <section class="bg-gray-900 p-6 rounded-lg shadow">
        <h2 class="text-lg font-semibold border-b border-gray-700 pb-2">Feed</h2>
    
        <% if (influencerPosts.length === 0) { %>
            <!-- Default Card when no posts exist -->
            <div class="bg-gray-800 p-4 rounded-lg mt-4 border-l-4 border-gray-600 shadow">
                <div class="flex items-center space-x-3">
                    <img src="/default-avatar.png" 
                         alt="Default Avatar" class="w-10 h-10 rounded-full border border-gray-700">
                    <div>
                        <p class="text-gray-400 text-sm font-semibold">FitnessPro Influencer</p>
                        <p class="text-gray-500 text-xs">Stay tuned for the latest updates!</p>
                    </div>
                </div>
                <p class="mt-3 text-gray-300">No posts yet! Our influencers are preparing something awesome. ðŸ’ª</p>
                <div class="mt-4">
                    <img src="/default-placeholder.jpg" alt="Default Post Image" 
                         class="w-full rounded-lg border border-gray-700 shadow">
                </div>
            </div>
        <% } else { %>
            <% influencerPosts.forEach(post => { %>
                <div class="bg-gray-800 p-4 rounded-lg mt-4 border-l-4 border-gray-600 shadow">
                    
                    <!-- User Info -->
                    <div class="flex items-center space-x-3">
                        <img src="data:image/jpeg;base64," 
                             alt="User Avatar" class="w-10 h-10 rounded-full border border-gray-700">
                        <div>
                            <p class="text-gray-400 text-sm font-semibold"><%= venkat %> (Influencer)</p>
                            <p class="text-gray-500 text-xs"><%= new Date(post.createdAt).toLocaleString() %></p>
                        </div>
                    </div>
    
                    <!-- Post Content -->
                    <p class="mt-3 text-gray-300"><%= post.content %></p>
    
                    <!-- Post Image (if available) -->
                    <% if (post.imageUrl) { %>
                        <div class="mt-4">
                            <img src="data:image/jpeg;base64," alt="Post Image" 
                                 class="w-full rounded-lg border border-gray-700 shadow">
                        </div>
                    <% } %>
    
                </div>
            <% }); %>
        <% } %>
    </section>
    

    <!-- User Posts -->
    <section class="bg-gray-900 p-6 rounded-lg shadow">
        <h2 class="text-lg font-semibold border-b border-gray-700 pb-2">Recent Posts</h2>

        <% if (userPosts.length === 0) { %>
            <p class="text-gray-500 mt-4">No posts yet. Be the first to share!</p>
        <% } else { %>
            <% userPosts.forEach(post => { %>
                <div class="bg-gray-800 p-4 rounded-lg mt-4">
                    <p class="text-gray-400 text-sm"><%= post.username %></p>
                    <p class="mt-2"><%= post.content %></p>
                    <p class="text-gray-500 text-xs mt-2"><%= new Date(post.createdAt).toLocaleString() %></p>
                </div>
            <% }); %>
        <% } %>
    </section>

    <!-- Post Form -->
    <section class="bg-gray-900 p-6 rounded-lg shadow">
        <h2 class="text-lg font-semibold">Share Your Journey</h2>
        <form action="/community" method="POST" class="mt-4">
            <textarea name="content" rows="3" required placeholder="Write something..." 
                class="w-full p-2 bg-gray-800 border border-gray-700 rounded text-gray-200"></textarea>
            <button type="submit" class="mt-4 w-full bg-gray-700 py-2 rounded-md hover:bg-gray-600">
                Post
            </button>
        </form>
    </section>

    <% if (user.isInfluencer) { %>
    <section class="bg-gray-900 p-6 rounded-lg shadow">
        <h2 class="text-lg font-semibold">Influencer Exclusive Post</h2>
        <form action="/community/influencer" method="POST" class="mt-4">
            <textarea name="content" rows="3" required placeholder="Share expert tips or motivation..." 
                class="w-full p-2 bg-gray-800 border border-gray-700 rounded text-gray-200"></textarea>
            <button type="submit" class="mt-4 w-full bg-gray-700 py-2 rounded-md hover:bg-gray-600">
                Post as Influencer
            </button>
        </form>
    </section>
    <% } %>
</main>

</body>
</html>
